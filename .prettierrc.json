{
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "endOfLine": "crlf",
  "overrides": [
    {
      "files": "{package,package-lock}.json",
      "options": {
        "endOfLine": "lf"
      }
    }
  ]
}
